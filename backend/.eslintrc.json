{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		/*"quotes": [2, "single"],*/
		"indent": ["warn", 2],
		"linebreak-style": ["error", "unix"],
		"semi": ["error", "never"]
	}
}
